#! /usr/bin/vvp
:ivl_version "0.9.6 " "(v0_9_6)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1108770 .scope module, "RAM_tb" "RAM_tb" 2 2;
 .timescale 0 0;
P_0x1105208 .param/l "DEPTH" 2 5, +C4<0100>;
P_0x1105230 .param/l "PERIOD" 2 6, +C4<01010>;
P_0x1105258 .param/l "WIDTH" 2 4, +C4<01000>;
v0x111c580_0 .var "address", 3 0;
v0x111c630_0 .net "clk", 0 0, v0x111c500_0; 1 drivers
v0x111c700_0 .var "data_in", 7 0;
v0x111c780_0 .net "data_out", 7 0, v0x111c0a0_0; 1 drivers
v0x111c860_0 .var "enable", 0 0;
v0x111c910_0 .var "wr_en", 0 0;
S_0x111c350 .scope module, "clk_gen_I" "clk_gen" 2 15, 3 2, S_0x1108770;
 .timescale 0 0;
P_0x111c448 .param/l "period" 3 3, +C4<01010>;
v0x111c500_0 .var "clk_out", 0 0;
S_0x1108860 .scope module, "RAM_I" "RAM" 2 20, 4 20, S_0x1108770;
 .timescale 0 0;
P_0x1106cc8 .param/l "DEPTH" 4 22, +C4<0100>;
P_0x1106cf0 .param/l "WIDTH" 4 23, +C4<01000>;
v0x1107020_0 .net "address", 3 0, v0x111c580_0; 1 drivers
v0x111bf60_0 .alias "clk", 0 0, v0x111c630_0;
v0x111c000_0 .net "data_in", 7 0, v0x111c700_0; 1 drivers
v0x111c0a0_0 .var "data_out", 7 0;
v0x111c150_0 .net "enable", 0 0, v0x111c860_0; 1 drivers
v0x111c1f0 .array "memory", 15 0, 7 0;
v0x111c2b0_0 .net "wr_en", 0 0, v0x111c910_0; 1 drivers
E_0x10e3c00 .event posedge, v0x111bf60_0;
    .scope S_0x111c350;
T_0 ;
    %set/v v0x111c500_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x111c350;
T_1 ;
    %delay 5, 0;
    %load/v 8, v0x111c500_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x111c500_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1108860;
T_2 ;
    %vpi_call 4 39 "$readmemh", "data.dat", v0x111c1f0;
    %end;
    .thread T_2;
    .scope S_0x1108860;
T_3 ;
    %wait E_0x10e3c00;
    %load/v 8, v0x111c150_0, 1;
    %jmp/0xz  T_3.0, 8;
    %load/v 8, v0x111c2b0_0, 1;
    %jmp/0xz  T_3.2, 8;
    %load/v 8, v0x111c000_0, 8;
    %ix/getv 3, v0x1107020_0;
    %jmp/1 t_0, 4;
    %ix/load 0, 8, 0; word width
    %ix/load 1, 0, 0; part off
    %assign/av v0x111c1f0, 0, 8;
t_0 ;
    %jmp T_3.3;
T_3.2 ;
    %ix/getv 3, v0x1107020_0;
    %load/av 8, v0x111c1f0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x111c0a0_0, 0, 8;
T_3.3 ;
T_3.0 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1108770;
T_4 ;
    %vpi_call 2 31 "$dumpfile", "RAM.vcd";
    %vpi_call 2 32 "$dumpvars", 1'sb0, S_0x1108770;
    %ix/load 0, 1, 0;
    %assign/v0 v0x111c860_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x111c910_0, 0, 0;
    %delay 100, 0;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 0;
    %delay 20, 0;
    %movi 8, 1, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 8;
    %delay 20, 0;
    %movi 8, 2, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 8;
    %delay 20, 0;
    %movi 8, 3, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 8;
    %delay 20, 0;
    %movi 8, 4, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 8;
    %delay 20, 0;
    %movi 8, 5, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0x111c910_0, 0, 1;
    %ix/load 0, 8, 0;
    %assign/v0 v0x111c700_0, 0, 1;
    %delay 20, 0;
    %movi 8, 5, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x111c580_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0x111c910_0, 0, 0;
    %delay 20, 0;
    %vpi_call 2 53 "$stop";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "RAM_tb.v";
    "../common/clk_gen.v";
    "RAM.v";
